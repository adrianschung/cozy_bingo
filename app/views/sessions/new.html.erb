 <%= form_tag '/login' do %>
  <h2>Log In</h2>
  
  <div class="field">
    <%= label_tag 'Username'%>
    <%= text_field_tag :username, params[:username] %>
  </div>

  <div class="field">
    <%= label_tag 'Password'%>
    <%= password_field_tag :password %></p>
  </div>

  <%= submit_tag "Log In" %>
<% end %>